{
  "name": "-",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vue-tsc && vite build",
    "clean": "rimraf node_modules package-lock.json dist",
    "dev": "vite",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,vue,css,scss,md}\"",
    "lint": "eslint --ext .js,.json,.ts,.tsx,.vue . --fix",
    "lint:ci": "eslint --ext .js,.json,.ts,.tsx,.vue . --no-fix",
    "lint:css": "stylelint \"src/**/*.{css,scss,vue}\" --fix",
    "lint:css:ci": "stylelint \"src/**/*.{css,scss,vue}\" --no-fix",
    "typecheck": "vue-tsc -p tsconfig.json"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "pinia": "^2.0.36",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vue-final-modal": "^4.4.2"
  },
  "devDependencies": {
    "@magister_zito/eslint-config-vue": "^0.15.0",
    "@magister_zito/prettier-config": "^0.0.5",
    "@magister_zito/stylelint-config": "^0.2.1",
    "@types/node": "^20.1.4",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "stylelint": "^15.6.1",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.3",
    "vite": "^4.3.2",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.4.2"
  }
}
